    "lint": "eslint src",
    "browserify": "browserify -d -e index.js -o dist/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "dir": "mkdir -p dist",
    "html": "cp index.html dist/",
    "build": "npm run dir && npm run html && npm run browserify",
