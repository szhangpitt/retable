  "devDependencies": {
    "babel-preset-es2015": "~6.22.0",
    "babelify": "~7.3.0",
    "browserify": "~14.0.0",
    "eslint": "~3.14.1",
    "eslint-plugin-babel": "~4.0.1",
    "gh-pages": "~0.12.0",
    "live-server": "~1.2.0",
    "watchify": "^3.9.0"
